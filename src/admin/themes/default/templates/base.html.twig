<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>{% block title %}{{ title }}{% endblock %}</title>
        {% block favicons %}
        <link rel="icon" type="image/png" href="{{ asset('assets/img/icon.png') }}">
        <link rel="apple-touch-icon" type="image/png" sizes="30x30" href="{{ asset('assets/img/Square30x30Logo.png') }}">
        <link rel="icon" type="image/png" sizes="30x30" href="{{ asset('assets/img/Square30x30Logo.png') }}">
        <link rel="apple-touch-icon" type="image/png" sizes="44x44" href="{{ asset('assets/img/Square44x44Logo.png') }}">
        <link rel="icon" type="image/png" sizes="44x44" href="{{ asset('assets/img/Square44x44Logo.png') }}">
        <link rel="apple-touch-icon" type="image/png" sizes="71x71" href="{{ asset('assets/img/Square71x71Logo.png') }}">
        <link rel="icon" type="image/png" sizes="71x71" href="{{ asset('assets/img/Square71x71Logo.png') }}">
        <link rel="apple-touch-icon" type="image/png" sizes="89x89" href="{{ asset('assets/img/Square89x89Logo.png') }}">
        <link rel="icon" type="image/png" sizes="89x89" href="{{ asset('assets/img/Square89x89Logo.png') }}">
        <link rel="apple-touch-icon" type="image/png" sizes="107x107" href="{{ asset('assets/img/Square107x107Logo.png') }}">
        <link rel="icon" type="image/png" sizes="107x107" href="{{ asset('assets/img/Square107x107Logo.png') }}">
        <link rel="apple-touch-icon" type="image/png" sizes="142x142" href="{{ asset('assets/img/Square142x142Logo.png') }}">
        <link rel="icon" type="image/png" sizes="142x142" href="{{ asset('assets/img/Square142x142Logo.png') }}">
        <link rel="apple-touch-icon" type="image/png" sizes="150x150" href="{{ asset('assets/img/Square150x150Logo.png') }}">
        <link rel="icon" type="image/png" sizes="150x150" href="{{ asset('assets/img/Square150x150Logo.png') }}">
        <link rel="apple-touch-icon" type="image/png" sizes="284x284" href="{{ asset('assets/img/Square284x284Logo.png') }}">
        <link rel="icon" type="image/png" sizes="284x284" href="{{ asset('assets/img/Square284x284Logo.png') }}">
        <link rel="apple-touch-icon" type="image/png" sizes="310x310" href="{{ asset('assets/img/Square310x310Logo.png') }}">
        <link rel="icon" type="image/png" sizes="310x310" href="{{ asset('assets/img/Square310x310Logo.png') }}">
        {% endblock %}
        {% block adminassets %}
            {% if adminbar is defined %}
                <link href="{{ asset('assets/admin/admin.css') }}" rel="stylesheet">
                <script src="{{ asset('assets/admin/admin.js')}}"></script>
            {% endif %}
        {% endblock %}
        {% block stylesheets %}
            <link href="{{ asset('assets/css/simple.min.css') }}" rel="stylesheet">
        {% endblock %}

        {% block javascripts %}
            <script src="{{ asset('assets/js/main.js') }}"></script>
        {% endblock %}
    </head>
    <body {% if adminbar is defined %} class="yolobar-opened" {% endif %}>
        {% if adminbar is defined %}{{ adminbar|raw }}{% endif %}
        {% block body %}{% endblock %}
    </body>
</html>
